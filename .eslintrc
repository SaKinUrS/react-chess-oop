{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "no-unused-vars":"warn",  
    "prefer-const": "error",
    "no-console":"warn",
    "quotes":["error","single"],
    "jsx-quotes": ["error", "prefer-double"],
    "indent": ["warn", 2],
    "max-len": ["error", {"code": 120}],
    "comma-dangle": ["error", "always-multiline"],
    "semi": ["warn", "always"],
    "import/order": ["error",{
      "groups": ["index", "sibling", "parent", "internal", "external", "builtin", "object", "type"],
      "newlines-between": "always-and-inside-groups"
    }]
  }
}